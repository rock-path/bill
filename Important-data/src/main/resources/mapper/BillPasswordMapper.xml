<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.psh.mapper.BillPasswordMapper">

    <sql id="table_field">
        #allSqlColumn
        ()
    </sql>
    <!--分页查询-->
    <select id="listByPage" resultType="com.psh.entity.response.ResBillPassword">
        select * from bill_password
        where deleted = 0
        <if test="null != param.softwareName and ''!= param.softwareName">
            and software_name like '%'||#{param.softwareName}||'%'
        </if>
        <if test="null != param.account and ''!= param.account">
            and account like '%'||#{param.account}||'%'
        </if>
        <if test="null != param.password and ''!= param.password">
            and password like '%'||#{param.password}||'%'
        </if>
        <if test="null != param.described and ''!= param.described">
            and described like '%'||#{param.described}||'%'
        </if>

    </select>
</mapper>
